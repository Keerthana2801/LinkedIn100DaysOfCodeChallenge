package javaPrograms;

public class FibonacciCalc {

public static void main(String[] args) {
int maxNumber=20;
System.out.println("Fibonacci series of "+maxNumber+" numbers: ");
for(int i=0; i<maxNumber; i++) {
System.out.print(calculateFibonacci(i)+" ");

}
}
public static int calculateFibonacci(int number) {
if(number==0) {
return 0;
}
if(number==1 || number==2 ) {
return 1;
}
return calculateFibonacci(number-2)+calculateFibonacci(number-1);

}
}
